#include<stdio.h>
#include<string.h>
#include<ctype.h>

#define SIZE 50

char stack[SIZE];
int top = -1;

void push(char ch){
  if(top == SIZE-1){
    printf("stack is overflow..!\n");
  }
 else{
   top++;
   stack[top]= ch;
  
    }
  
}
int pop()
int main(){
  printf("i am a stack..! \n");
  push("x");
  return 0;
}
